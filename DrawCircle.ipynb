   "source": [
    "import turtle as tl\n",
    "screen = tl.Screen()\n",
    "screen.bgcolor(\"green\")\n",
    "pen = tl.Turtle()\n",
    "pen.speed(10)\n",
    "pen.pensize(5)\n",
    "pen.pencolor(\"red\")\n",
    "pen.fillcolor(\"yellow\")\n",
    "pen.begin_fill()\n",
    "pen.penup()\n",
    "pen.goto(-100,50)\n",
    "pen.pendown()\n",
    "pen.circle(100)\n",
    "pen.end_fill()\n",
    "tl.done()"
   ]
  }
 ],
